{
    "model":{

        "darknet":{
            "darknet_cfg":    "tmp/yolo3_model/FT-Prohibit-yolov3-tiny-anchor5-tt100k-512-test.cfg",
            "head":           "yolov3"
        },

        "experiment":{
            "backcbone":      "yolov3-tiny",
            "head":           "yolov3"
        },

        "weights":{
            "format":         "darknet",
            "pretrain":       "/code/weights/yolov3-tiny.weights",
            "resume":         "False",
            "transfer":       15,
            "freeze":         "False"
        },

        "parameters":{
            "dictfile":       "/code/data/sign/FT-prohibit.names",
            "autoevolve":     "False",
            "notest":         "False",
            "nosave":         "False",
            "save_interval":  1,
            "debug":          "False"
        }

    },


    "train":{
        
        "name":               "TC-sign-pytorch-yolov3",

        "data":{
            "format":         "darknet",
            "txtpath":        "/data/sign_detect/mini/ImageSets/Main/train-fullpath.txt",
            "augment":        "True"
        },

        "parameters":{
            "max_epoch":      100,
            "batch":          16,
            "size":           512,
            "learning_rate":  0.001,
            "lr_scheduler":   "",
            "momentum":       0.9127,
            "weight_decay":   0.0004841,
            "gpus":           ""
        }

    },

    "test":{

        "data":{
            "format":         "darknet",
            "txtpath":        "/data/sign_detect/mini/ImageSets/Main/train-fullpath.txt"
        },

        "parameters":{
            "start_epoch":    1,
            "interval":       1,            
            "batch":          2,
            "size":           512
        }
    },

    "evaluate":{

        "model":{
            "weights_path":   "/code/weights/TC-sign-pytorch-yolov3/best.pt",
            "conf_thresh":    0.5,
            "nms_thresh":     0.5
        },

        "data":{
            "name":           "TC-test",
            "dirpath":        "/data/sign_detect/mini/JPEGImages/test",
            "isimages":       "True"
        },

        "parameters":{
            "outdir":         "/code/output",
            "size":           512,
            "save_images":    "True",
            "gpus":           ""
        }

    }
}